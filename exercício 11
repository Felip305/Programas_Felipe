#include <stdio.h>
#include <math.h>
#include <locale.h>

int main(){

    setlocale(LC_ALL,"portuguese");

    int n1, n2, n3;

        printf("Digite o primeiro numero: ");
        scanf("%d", &n3);

        printf("Digite o segundo numero: ");
        scanf("%d", &n2);

        printf("Digite o terceiro numero: ");
        scanf("%d", &n1);


    if (n3 < n2 && n3 < n1) {
        if (n2 < n3)
            printf("\n Aqui estam esses números em ordem crescente: %d, %d, %d\n", n3, n2, n1);
        else
            printf("\n Aqui estam esses números em ordem crescente: %d, %d, %d\n", n3, n1, n2);
    } else if (n2 < n3 && n2 < n1) {
        if (n3 < n1)
            printf("\n Aqui estam esses números em ordem crescente: %d, %d, %d\n", n2, n3, n1);
        else
            printf("\n Aqui estam esses números em ordem crescente: %d, %d, %d\n", n2, n1, n3);
    } else {
        if (n3 < n2)
            printf("\n Aqui estam esses números em ordem crescente: %d, %d, %d\n", n1, n3, n2);
        else
            printf("\n Aqui estam esses números em ordem crescente: %d, %d, %d\n", n1, n2, n3);
    }


    return 0;
}
