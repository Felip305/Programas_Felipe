#include <stdio.h>

int main(){

	int tab;
	
	printf("Qual tabuada quer visualizar?: ");
	scanf("%d", &tab);
	printf("%d x %2d = %2d\n", tab, 1, tab*1);
	printf("%d x %2d = %2d\n", tab, 2, tab*2);
	printf("%d x %2d = %2d\n", tab, 3, tab*3);
	printf("%d x %2d = %2d\n", tab, 4, tab*4);
	printf("%d x %2d = %2d\n", tab, 5, tab*5);
	printf("%d x %2d = %2d\n", tab, 6, tab*6);
	printf("%d x %2d = %2d\n", tab, 7, tab*7);
	printf("%d x %2d = %2d\n", tab, 8, tab*8);
	printf("%d x %2d = %2d\n", tab, 9, tab*9);
	printf("%d x %2d = %2d\n", tab, 10, tab*10);
	
	
	return 0;
}
